{
    "name": "hexlet/code",
    "description": "education practice",
    "type": "project",
    "authors": [
        {
            "name": "Vlad Werner",
            "email": "inqast@gmail.com"
        }
    ],
    "license": "MIT",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Php\\Package\\": "src",
            "Php\\Package\\Tests\\": "tests"
        },
        "files": [
            "src/Differ.php",
            "src/Tree.php",
            "src/Parser.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Json.php"
        ]
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "docopt/docopt": "^1.0",
        "funct/funct": "^1.5",
        "wp-cli/php-cli-tools": "^0.11.11",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.5"
    }
}
